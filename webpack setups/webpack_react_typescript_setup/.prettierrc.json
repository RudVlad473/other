{
    "semi": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "organizeImportsSkipDestructiveCodeActions": true
}
